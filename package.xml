<?xml version="1.0" encoding="UTF-8"?>
<package packagerversion="1.8.0" version="2.0" xmlns="http://pear.php.net/dtd/package-2.0" xmlns:tasks="http://pear.php.net/dtd/tasks-1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0
    http://pear.php.net/dtd/tasks-1.0.xsd
    http://pear.php.net/dtd/package-2.0
    http://pear.php.net/dtd/package-2.0.xsd">
 <name>BundleFu</name>
 <channel>pear.dotsunited.de</channel>
 <summary>Simple CSS and JavaScript bundling</summary>
 <description>BundleFu is a PHP 5.3+ library which bundles multiple CSS and JavaScript files into a big package and sends it out at once.</description>
 <lead>
  <name>Jan Sorgalla</name>
  <user>jsor</user>
  <email>jan.sorgalla@dotsunited.de</email>
  <active>yes</active>
 </lead>
 <date>2012-02-28</date>
 <version>
  <release>0.9.2</release>
  <api>0.9.2</api>
 </version>
 <stability>
  <release>beta</release>
  <api>beta</api>
 </stability>
 <license uri="https://github.com/dotsunited/BundleFu/blob/master/LICENSE">New BSD License</license>
 <notes>https://github.com/dotsunited/BundleFu#readme</notes>
 <contents>
  <dir name="/">
   <file name="LICENSE" role="doc" />
   <file name="README.markdown" role="doc" />
   <dir name="src">
    <dir name="DotsUnited">
     <dir name="BundleFu">
      <dir name="Filter">
       <file install-as="DotsUnited/BundleFu/Filter/Callback.php" name="Callback.php" role="php">
        <tasks:replace from="@package_version@" to="version" type="package-info" />
       </file>
       <file install-as="DotsUnited/BundleFu/Filter/ClosureCompilerService.php" name="ClosureCompilerService.php" role="php">
        <tasks:replace from="@package_version@" to="version" type="package-info" />
       </file>
       <file install-as="DotsUnited/BundleFu/Filter/FilterChain.php" name="FilterChain.php" role="php">
        <tasks:replace from="@package_version@" to="version" type="package-info" />
       </file>
       <file install-as="DotsUnited/BundleFu/Filter/FilterInterface.php" name="FilterInterface.php" role="php">
        <tasks:replace from="@package_version@" to="version" type="package-info" />
       </file>
      </dir>
      <file install-as="DotsUnited/BundleFu/Bundle.php" name="Bundle.php" role="php">
       <tasks:replace from="@package_version@" to="version" type="package-info" />
      </file>
      <file install-as="DotsUnited/BundleFu/CssUrlRewriter.php" name="CssUrlRewriter.php" role="php">
       <tasks:replace from="@package_version@" to="version" type="package-info" />
      </file>
      <file install-as="DotsUnited/BundleFu/Factory.php" name="Factory.php" role="php">
       <tasks:replace from="@package_version@" to="version" type="package-info" />
      </file>
      <file install-as="DotsUnited/BundleFu/FileList.php" name="FileList.php" role="php">
       <tasks:replace from="@package_version@" to="version" type="package-info" />
      </file>
     </dir>
    </dir>
   </dir>
  </dir>
 </contents>
 <dependencies>
  <required>
   <php>
    <min>5.3.0</min>
   </php>
   <pearinstaller>
    <min>1.9.1</min>
   </pearinstaller>
   <extension>
    <name>pcre</name>
   </extension>
   <extension>
    <name>spl</name>
   </extension>
  </required>
 </dependencies>
 <phprelease/>
</package>
