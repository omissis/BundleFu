{
    "name": "omissis/bundlefu",
    "type": "library",
    "description": "BundleFu is a PHP 5.3+ library which bundles multiple css/javascript files into a big package and sends it out at once",
    "keywords": ["css", "javascript", "bundling", "asset", "rackspace", "cloudfiles"],
    "homepage": "https://github.com/omissis/BundleFu",
    "license": "New BSD",
    "authors": [
        {
            "name": "Jan Sorgalla",
            "email": "jan.sorgalla@dotsunited.de",
            "homepage": "http://dotsunited.de"
        },
        {
            "name": "Claudio Beatrice",
            "email": "claudi0.beatric3@gmail.com",
            "homepage": "http://silent-voice.org"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "rackspace/cloudfiles",
                "version" : "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/rackspace/php-cloudfiles.git",
                    "reference": "master"
                },
                "dist": {
                    "url": "https://github.com/rackspace/php-cloudfiles/zipball/master",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "rackspace/cloudfiles": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "DotsUnited\\BundleFu": "src/"
        }
    },
    "include-path": ["lib/", "vendor/rackspace/cloudfiles"]
}
